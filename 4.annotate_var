bgzip -k ./variants/out.vcf
tabix ./variants/out.vcf.gz

bcftools annotate --threads 12 --annotations /home/bio/db/GCF_000001405.40.gz --columns CHROM,ID,INFO ./variants/out.vcf.gz > ./variants/annotated.vcf
bcftools view -e 'ID="."' ./variants/annotated.vcf > ./variants/annotated_only_with_id.vcf
bcftools query -H -f '%CHROM %POS  %ID  %REF  %ALT %INFO/GENEINFO [ %TGT]\n' ./variants/annotated_only_with_id.vcf -o ./variants/with_id.vcf
